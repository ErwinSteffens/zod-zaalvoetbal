{
    "name": "zod-zaalvoetbal",
    "private": true,
    "description": "Gatsby website for a local soccer competition",
    "version": "0.1.0",
    "license": "MIT",
    "scripts": {
        "build": "gatsby build",
        "clean": "gatsby clean",
        "deploy": "node deploy",
        "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
        "import": "cd ../import && npm run import",
        "pdf": "node scripts/generate-pdf.js",
        "release": "run-s build pdf build",
        "serve": "gatsby serve",
        "start": "gatsby develop",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "bootstrap-scss": "^4.6.2",
        "classnames": "^2.3.2",
        "gatsby": "4.24.5",
        "gatsby-plugin-gtag": "^1.0.13",
        "gatsby-plugin-manifest": "^4.24.0",
        "gatsby-plugin-pnpm": "1.2.10",
        "gatsby-plugin-sass": "^5.24.0",
        "gatsby-source-filesystem": "^4.24.0",
        "gatsby-transformer-json": "^4.24.0",
        "gatsby-transformer-remark": "^5.25.1",
        "gatsby-transformer-yaml": "^4.24.0",
        "immutable": "^4.1.0",
        "rambda": "7.3.0",
        "react": "18.2.0",
        "react-bootstrap": "^1.6.6",
        "react-dom": "18.2.0",
        "react-recaptcha-google": "^1.1.1",
        "react-toggle": "^4.1.3"
    },
    "devDependencies": {
        "@types/node": "18.11.7",
        "@types/ramda": "0.28.18",
        "@types/react": "18.0.24",
        "@types/react-dom": "18.0.8",
        "dotenv": "^16.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "2.7.1",
        "puppeteer": "^19.2.0",
        "rimraf": "^3.0.2",
        "sass": "^1.55.0",
        "typescript": "4.8.4",
        "wait-on": "^6.0.1"
    }
}
